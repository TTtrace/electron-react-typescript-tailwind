{
  "name": "bean-electron-react-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main",
  "scripts": {
    "dev:react": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron .",
